<div class="container">
  <div class="row">
    <div class="col-md-5">
        <table class="table table-striped">
          <thead>
            <td><strong>Movie Name</strong></td>
            <td><strong>Movie URL</strong></td>
            <td><strong>Comments</strong></td>
          </thead>
          {{#each doc}}
          <tr>
            <td>{{this.movieName}}</td>
            <td>{{this.movieUrl}}</td>
            <td><button class="btn btn-default" type="submit">Comment</button></td>
          </tr>
        {{/each}}
        </table>
    </div>
  </div>
</div>
